{
  "name": "saaksin-ngx-dropzone-wrapper",
  "description": "Angular wrapper library for Dropzone",
  "bugs": "https://github.com/SaAkSin/ngx-dropzone-wrapper/issues",
  "license": "MIT",
  "version": "4.0.7",
  "main": "bundles/saaksin-ngx-dropzone-wrapper.umd.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "init": "npm install",
    "build": "npm run build:umd && npm run build:aot",
    "build:js": "ngc -p src/tsconfig.json",
    "build:css": "node-sass src/ -o dist/ --source-map=true",
    "build:html": "copyfiles -f src/lib/*.html dist/lib",
    "build:umd": "webpack --config config/webpack.config.js",
    "build:aot": "npm run build:js && npm run build:css && npm run build:html",
    "example": "npm run example:bundles && npm run example:dist && npm run example:lib",
    "example:bundles": "copyfiles -f bundles/* example/node_modules/saaksin-ngx-dropzone-wrapper/bundles",
    "example:dist": "copyfiles -f dist/* example/node_modules/saaksin-ngx-dropzone-wrapper/dist",
    "example:lib": "copyfiles -f dist/lib/* example/node_modules/saaksin-ngx-dropzone-wrapper/dist/lib",
    "release": "npm version -m 'New release with version %s'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SaAkSin/ngx-dropzone-wrapper.git"
  },
  "dependencies": {
    "dropzone": "~4.3.0"
  },
  "devDependencies": {
    "@angular/common": "^4.0.2",
    "@angular/compiler": "^4.0.2",
    "@angular/compiler-cli": "^4.0.2",
    "@angular/core": "^4.0.2",
    "@angular/http": "^4.0.2",
    "@angular/platform-browser": "^4.0.2",
    "@types/node": "^6.0.46",
    "angular2-inline-template-style": "~1.0.0",
    "angular2-template-loader": "~0.6.0",
    "awesome-typescript-loader": "~3.1.0",
    "codelyzer": "~2.0.0",
    "core-js": "~2.4.0",
    "cpx": "~1.5.0",
    "node-sass": "~4.5.0",
    "raw-loader": "~0.5.0",
    "rxjs": "~5.1.0",
    "sass-loader": "~6.0.0",
    "string-replace-loader": "~1.1.0",
    "strip-sourcemap-loader": "~0.0.0",
    "stylelint": "~7.9.0",
    "stylelint-config-standard": "~13.0.0",
    "tslint": "~4.5.0",
    "typescript": "^2.4.0",
    "watch": "~1.0.0",
    "webpack": "~2.3.0",
    "zone.js": "^0.8.4",
    "copyfiles": "^1.2.0"
  }
}
